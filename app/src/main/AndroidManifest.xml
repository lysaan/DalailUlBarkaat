<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <application
        android:allowBackup="true"
        android:configChanges="locale|screenSize|orientation|keyboardHidden"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/override_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="sensor"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:usesCleartextTraffic="false"
        tools:replace="android:label">
        <activity
            android:name="findmyphone.findmyphonebyclap.phonefindergo.activities.PhoneFindGoMainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="findmyphone.findmyphonebyclap.phonefindergo.activities.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="findmyphone.findmyphonebyclap.phonefindergo.BaseActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
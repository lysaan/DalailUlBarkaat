    <!-- Dos Tips -->
    <string name="tip_1_title">Keep Battery Level Between 20%-80%</string>
    <string name="tip_1_description">Avoid full charges (100%) and letting the battery drop to 0%. This reduces battery wear.</string>

    <string name="tip_2_title">Use the Original Charger</string>
    <string name="tip_2_description">Always use the charger and cable that came with your device or a certified equivalent.</string>

    <string name="tip_3_title">Enable Adaptive Battery and Brightness</string>
    <string name="tip_3_description">These features optimize battery usage based on your habits and save power by adjusting performance intelligently.</string>

    <string name="tip_4_title">Close Unnecessary Apps</string>
    <string name="tip_4_description">Use the \'Force Stop\' option for apps running unnecessarily in the background.</string>

    <string name="tip_5_title">Keep the Software Updated</string>
    <string name="tip_5_description">Updates often include battery and performance optimization.</string>

    <string name="tip_6_title">Use Battery Saver Mode</string>
    <string name="tip_6_description">Activate battery saver when you\'re running low on charge to extend usage.</string>

    <string name="tip_7_title">Keep Apps Updated</string>
    <string name="tip_7_description">Developers optimize apps for better battery usage in updates.</string>

    <string name="tip_8_title">Maintain Moderate Brightness Levels</string>
    <string name="tip_8_description">Use auto-brightness or manually keep the screen brightness low when possible.</string>

    <string name="tip_9_title">Limit Background Activities</string>
    <string name="tip_9_description">Restrict background data and activity for less-used apps.</string>

    <string name="tip_10_title">Remove Unused Apps and Widgets</string>
    <string name="tip_10_description">Keep your device uncluttered to reduce background processes and save battery.</string>

    <string name="tip_11_title">Avoid Extreme Temperatures</string>
    <string name="tip_11_description">Keep your device out of very hot or cold environments, as extreme temperatures degrade battery health.</string>

    <!-- Don'ts Tips -->
    <string name="tip_12_title">Avoid Overnight Charging</string>
    <string name="tip_12_description">Prolonged charging when the battery is already full can lead to overheating and degrade the battery over time.</string>

    <string name="tip_13_title">Don’t Use Fast Charging All the Time</string>
    <string name="tip_13_description">While convenient, constant fast charging can stress the battery and shorten its lifespan.</string>

    <string name="tip_14_title">Avoid Heavy Gaming or Streaming While Charging</string>
    <string name="tip_14_description">This increases heat and puts a strain on the battery.</string>

    <string name="tip_15_title">Don’t Use Third-Party or Non-Certified Chargers</string>
    <string name="tip_15_description">They may not regulate voltage properly, causing damage to your battery.</string>

    <string name="tip_16_title">Avoid Draining the Battery to 0% Frequently</string>
    <string name="tip_16_description">This can lead to deep discharges that damage the battery.</string>

    <string name="tip_17_title">Don’t Use High-Performance Modes Unnecessarily</string>
    <string name="tip_17_description">Features like high-refresh-rate displays and maximum resolutions consume extra power.</string>

    <string name="tip_18_title">Avoid Leaving Apps Open in the Background</string>
    <string name="tip_18_description">Too many background apps can drain your battery quickly and slow down your device.</string>

    <string name="tip_19_title">Donnot Ignore Cached Data</string>
    <string name="tip_19_description">Regularly clear cached data from apps to free up space and maintain efficiency.</string>

    <string name="tip_20_title">Don’t Charge in a Confined Space</string>
    <string name="tip_20_description">Avoid charging on beds, under pillows, or in cases that don’t allow heat dissipation.</string>

    <string name="tip_21_title">Avoid Unnecessary Syncing</string>
    <string name="tip_21_description">Turn off auto-sync for email or social media apps you don’t frequently use.</string>

    <!-- Bonus Efficiency Tips -->
    <string name="tip_22_title">Use Dark Mode</string>
    <string name="tip_22_description">If your device has an OLED screen, it saves battery by reducing pixel power.</string>

    <string name="tip_23_title">Reboot Weekly</string>
    <string name="tip_23_description">Clears temporary system processes that may affect performance.</string>

    <string name="tip_24_title">Keep the Device Clean</string>
    <string name="tip_24_description">Physically clean the charging port and vents for better charging and cooling.</string>

